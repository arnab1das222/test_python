pipeline {
  agent any
  stages {
    stage ('Checkout') {
      steps {
        echo 'I am in Checkout'
      }
    }
    stage ('Build') {
      steps {
        echo 'I am in Build'
      }
    }
    stage ('Unit Test') {
      steps {
        echo 'I am in Unit Test'
      }
    }
    stage ('Lint') {
      steps {
        echo 'I am in Lint'
      }
    }
    stage ('Static Code') {
      steps {
        echo 'I am in Static Code'
      }
    }
    stage ('Package') {
      steps {
        echo 'I am in Static Code'
      }
    }
    stage ('Publish') {
      steps {
        echo 'I am in Static Code'
      }
    }
  }
}
